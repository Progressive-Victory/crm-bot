  "private": true,
  "scripts": {
    "run": "node ./dist/index.js",
    "lint": "eslint . --fix --ext .ts",
    "build": "swc ./src --out-dir dist",
    "start": "yarn build && yarn run run",
    "test": "jest",
    "dev": "yarn start | pino-pretty -i pid,hostname -t 'yyyy-mm-dd HH:MM:ss'",
    "pm2": "yarn build && pm2 start dist/index.js --name crm-bot"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@types/jest": "^28.1.6",
