{
	"name": "crm-bot",
	"version": "2.1.0",
	"repository": "git@github.com:Progressive-Victory/crm-bot.git",
	"author": "Isabella <isabellafj97@gmail.com>",
	"license": "MIT",
	"private": true,
	"scripts": {
		"run": "node ./dist/index.js",
		"build": "rimraf dist && swc ./src --out-dir dist",
		"start": "yarn build && yarn run run",
		"test": "jest",
		"dev": "yarn start | pino-pretty -i pid,hostname -t 'yyyy-mm-dd HH:MM:ss'",
		"prepm2": "yarn build",
		"pm2": "pm2 start dist/index.js --name crm-bot",
		"prepare": "husky install",
		"lint": "eslint {src,test}/**/*.{js,ts,jsx,tsx} --fix --no-error-on-unmatched-pattern",
		"style": "prettier-eslint \"src/**/*.ts\" --write && yarn lint"
	},
	"engines": {
		"node": ">=16.9.0"
	},
	"dependencies": {
		"@discordjs/builders": "^1.5.0",
		"@discordjs/rest": "^1.0.0",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.3.14",
		"@types/mongodb": "^4.0.7",
		"discord-api-types": "^0.36.3",
		"discord.js": "^14.8.0",
		"dotenv": "^16.0.1",
		"mongodb": "^4.8.0",
		"node-fetch": "2.6.7",
		"pino": "^8.3.0",
		"pino-pretty": "^8.1.0",
		"rimraf": "^5.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.3",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.21.0",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.3.14",
		"@types/jest": "^28.1.6",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"babel-jest": "^29.5.0",
		"eslint": "^8.13.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"prettier": "^2.8.8",
		"prettier-eslint": "^15.0.1",
		"prettier-eslint-cli": "^7.1.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	}
}
