    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@discordjs/rest": "^1.0.0",
    "@types/mongodb": "^4.0.7",
    "discord-api-types": "^0.36.3",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.1",
    "mongodb": "^4.8.0",
